{
  "presets": [
    "@babel/env",
    "@babel/preset-react"
  ],
  "rootMode": "upward",
  "plugins": [
    "@babel/plugin-syntax-jsx",
    "@babel/plugin-transform-runtime",
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "@babel/plugin-proposal-private-methods",
      {
        "loose": true
      }
    ]
  ],
  "ignore": [
    "resources/modules/node_modules"
  ]
}
